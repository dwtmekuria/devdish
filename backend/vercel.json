{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CORS_ORIGINS": "https://devdish.vercel.app,http://localhost:5173",
    "FRONTEND_URL": "https://devdish.vercel.app"

  }
}